<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <decisions>
        <name>License_Found</name>
        <label>License Found</label>
        <locationX>348</locationX>
        <locationY>152</locationY>
        <defaultConnector>
            <targetReference>Sorry_About_The_Error</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Fail</defaultConnectorLabel>
        <rules>
            <name>License_Exists</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Lookup_License</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Thank_You</targetReference>
            </connector>
            <label>License Exists</label>
        </rules>
        <rules>
            <name>License_Does_Not_Exist</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Lookup_License</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>No_License</targetReference>
            </connector>
            <label>License Does Not Exist</label>
        </rules>
    </decisions>
    <dynamicChoiceSets>
        <name>AccountChoice</name>
        <dataType>String</dataType>
